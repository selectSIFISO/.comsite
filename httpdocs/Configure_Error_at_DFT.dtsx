<![CDATA[using System;
using System.Data;
using System.Data.Sql;
using System.Data.SqlTypes;
using System.Data.SqlClient;
using Microsoft.SqlServer.Server;
using Microsoft.SqlServer.Dts.Tasks;
using Microsoft.SqlServer.Dts.Runtime;
using Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask;
using Microsoft.SqlServer.Dts.Pipeline.Wrapper;
using RuntimeWrapper = Microsoft.SqlServer.Dts.Runtime.Wrapper;
using System.IO;

namespace ST_f1d958893b584785957c8d1aa79604ef.csproj
{
    [System.AddIn.AddIn("ScriptMain", Version = "1.0", Publisher = "", Description = "")]
    public partial class ScriptMain : Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
    {

        #region VSTA generated code
        enum ScriptResults
        {
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        };
        #endregion

        public void Main()
        {
            // Add an instance of application
            Application app = new Application();
            Package importPackage = new Package();
            
            importPackage.Name = "Red Thing";
            importPackage.Description = "Red Thing";

            // Add the Data Flow Task
            Executable DTF_exec = importPackage.Executables.Add("STOCK:PipelineTask");
            TaskHost taskHost = DTF_exec as TaskHost;
            MainPipe dataFlowTask = (MainPipe)taskHost.InnerObject;
            taskHost.Name = "Dynamic Data Flow Task";
            taskHost.Properties["DefaultBufferMaxRows"].SetValue(taskHost, "1000000");

            // Add the SQL OLE-DB Source connection
            ConnectionManager connectionManagerOleDbSRC = importPackage.Connections.Add("OLEDB");
            connectionManagerOleDbSRC.ConnectionString = string.Format("Provider=SQLOLEDB.1;Data Source={0};Initial Catalog={1};Integrated Security=SSPI;", "localhost", "Config");
            connectionManagerOleDbSRC.Name = "OLEDB SRC";

            //Add and Configure Data Flow Task Source Component
            IDTSComponentMetaData100 component = dataFlowTask.ComponentMetaDataCollection.New();
            component.Name = "OLEDBSource";
            component.ComponentClassID = "DTSAdapter.OleDbSource.2";

            CManagedComponentWrapper instance = component.Instantiate();
            instance.ProvideComponentProperties();
            
            component.RuntimeConnectionCollection[0].ConnectionManagerID = connectionManagerOleDbSRC.ID;
            component.RuntimeConnectionCollection[0].ConnectionManager = DtsConvert.ToConnectionManager90(connectionManagerOleDbSRC);
            
            connectionManagerOleDbSRC.AcquireConnection(null);

            instance.SetComponentProperty("AlwaysUseDefaultCodePage", Convert.ToBoolean("True"));
            instance.SetComponentProperty("AccessMode", 2);
            instance.SetComponentProperty("SqlCommand", "SELECT [StreamID],[StreamDesc],[RelationshipTypeCode],[Folder],[FullPkgName] FROM [Config].[dbo].[Streams]");
            instance.AcquireConnections(null);
            instance.ReinitializeMetaData();
            instance.ReleaseConnections();

            IDTSComponentMetaData100 componentDerivedTransform = dataFlowTask.ComponentMetaDataCollection.New();
            componentDerivedTransform.ComponentClassID = "DTSTransform.DerivedColumn";
            componentDerivedTransform.Name = "Package Derived Columns";

            CManagedComponentWrapper DesignDerivedTransformColumns = componentDerivedTransform.Instantiate();
            DesignDerivedTransformColumns.ProvideComponentProperties();

            componentDerivedTransform.InputCollection[0].ExternalMetadataColumnCollection.IsUsed = false;
            componentDerivedTransform.InputCollection[0].HasSideEffects = false;

            DesignDerivedTransformColumns.AcquireConnections(null);
            DesignDerivedTransformColumns.ReinitializeMetaData();
            DesignDerivedTransformColumns.ReleaseConnections();

            IDTSPath100 SourceToDerivedPath = dataFlowTask.PathCollection.New();
            SourceToDerivedPath.AttachPathAndPropagateNotifications(component.OutputCollection[0], componentDerivedTransform.InputCollection[0]);

            IDTSOutputColumn100 myColTaskName = componentDerivedTransform.OutputCollection[0].OutputColumnCollection.New();
            myColTaskName.Name = "TaskName";
            myColTaskName.SetDataTypeProperties(Microsoft.SqlServer.Dts.Runtime.Wrapper.DataType.DT_WSTR, 100, 0, 0, 0);
            myColTaskName.ExternalMetadataColumnID = 0;
            myColTaskName.ErrorRowDisposition = DTSRowDisposition.RD_FailComponent;
            myColTaskName.TruncationRowDisposition = DTSRowDisposition.RD_FailComponent;

            IDTSCustomProperty100 myPropTaskName = myColTaskName.CustomPropertyCollection.New();
            myPropTaskName.Name = "Expression";
            myPropTaskName.Value = "@[System::TaskName]";

            myPropTaskName = myColTaskName.CustomPropertyCollection.New();
            myPropTaskName.Name = "FriendlyExpression";
            myPropTaskName.Value = "@[System::TaskName]";
                        
            //Create the input columns for the transformation component
            IDTSInput100 input = componentDerivedTransform.InputCollection[0];
            IDTSVirtualInput100 derivedInputVirtual = input.GetVirtualInput();
            input.ErrorRowDisposition = DTSRowDisposition.RD_NotUsed;
            input.ErrorOrTruncationOperation = "";
            DesignDerivedTransformColumns.ReleaseConnections();

            IDTSComponentMetaData100 componentOleDBComm = dataFlowTask.ComponentMetaDataCollection.New();
            componentOleDBComm.ComponentClassID = "DTSTransform.OLEDBCommand";
            componentOleDBComm.Name = "Insert Error into OLEDBCommand";
            componentOleDBComm.Description = "Insert Error into OLEDBCommand";
                        
            CManagedComponentWrapper DesignOleDBCommColumns = componentOleDBComm.Instantiate();
            DesignOleDBCommColumns.ProvideComponentProperties();

            componentOleDBComm.RuntimeConnectionCollection[0].ConnectionManagerID = connectionManagerOleDbSRC.ID;
            componentOleDBComm.RuntimeConnectionCollection[0].ConnectionManager = DtsConvert.ToConnectionManager90(connectionManagerOleDbSRC);
            
            DesignOleDBCommColumns.SetComponentProperty("SqlCommand", "insert into [Config].[dbo].[Table_1] values (99)");
            //DesignOleDBCommColumns.ReinitializeMetaData();

            IDTSPath100 SourceToOleDBCommandError = dataFlowTask.PathCollection.New();
            SourceToOleDBCommandError.AttachPathAndPropagateNotifications(component.OutputCollection[1], componentOleDBComm.InputCollection[0]);

            IDTSInput100 DerivedColumnInput = componentOleDBComm.InputCollection[0];
            IDTSVirtualInput100 DerivedColumnVirtualInput = DerivedColumnInput.GetVirtualInput();
            IDTSVirtualInputColumnCollection100 DerivedColumnVirtualInputColumns = DerivedColumnVirtualInput.VirtualInputColumnCollection;
    
            //Save package to file system
            app.SaveToXml(@"C:\Temp\pkg\RedThing.dtsx", importPackage, null);

            Dts.TaskResult = (int)ScriptResults.Success;
                }
            }
    }]]></ProjectItem><ProjectItem Name="\properties\resources.designer.cs">
